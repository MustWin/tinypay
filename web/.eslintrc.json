{
  "extends": "eslint:recommended",
  "env": {
    "node": true,
    "browser": true,
    "jquery": true
  },
  "rules": {
    "quotes": [
      "error",
      "double"
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "no-console": "off"
  },
  "globals": {
    "Backbone": true,
    "DomainMicropay": true,
    "MP": true,
    "Promise": true,
    "web3": true,
    "_": true
  }
}
